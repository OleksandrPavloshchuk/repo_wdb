<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>diagram-builder.web</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	<context-param>
		<description>List of block plugins</description>
		<param-name>block-plugins</param-name>
		<param-value><![CDATA[
  		org.wdbuilder.plugin.common.CommonBlockPluginFacade,
  		org.wdbuilder.plugin.icon.IconBlockPluginFacade
  	]]></param-value>
	</context-param>
    <context-param>
        <description>List of link plugins</description>
        <param-name>link-plugins</param-name>
        <param-value><![CDATA[
        org.wdbuilder.plugin.defaultlink.DefaultLinkPluginFacade
    ]]></param-value>
    </context-param>
</web-app>
